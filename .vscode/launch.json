{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next: Server",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "skipFiles": ["<node_internals>/**"]
    },
    {
      "name": "Next: Client",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
